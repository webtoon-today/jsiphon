{
  "name": "jsiphon",
  "version": "0.2.3",
  "description": "An append-only streaming JSON parser. Parse incomplete JSON as it streams in - data is never removed or mutated, only extended.",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rm -rf dist",
    "build": "npm run clean && node build.mjs && tsc -p tsconfig.build.json",
    "test": "vitest",
    "test:run": "vitest run",
    "prepublishOnly": "npm run test:run && npm run build"
  },
  "keywords": [
    "jsiphon",
    "json",
    "streaming",
    "parser",
    "partial",
    "incomplete",
    "append-only",
    "llm",
    "async"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/webtoon-today/jsiphon.git"
  },
  "bugs": {
    "url": "https://github.com/webtoon-today/jsiphon/issues"
  },
  "homepage": "https://github.com/webtoon-today/jsiphon#readme",
  "devDependencies": {
    "esbuild": "^0.27.2",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  }
}
